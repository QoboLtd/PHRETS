{
  "name": "qoboltd/phrets",
  "type": "library",
  "license": "MIT",
  "description": "RETS library in PHP",
  "keywords": [
    "rets",
    "mls"
  ],
  "homepage": "https://github.com/QoboLtd/PHRETS",
  "authors": [
    {
      "name": "Qobo Ltd",
      "email": "hello@qobogroup.com",
      "homepage": "https://qobogroup.com/",
      "role": "Developer"
    },
    {
      "name": "Troy Davisson",
      "email": "troy.davisson@gmail.com",
      "homepage": "http://troda.com",
      "role": "Developer"
    },
    {
      "name": "Max Lispky",
      "email": "maxlipsky@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "ext-simplexml": "*",
    "guzzlehttp/guzzle": "^7.2",
    "league/csv": "^9.7",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "monolog/monolog": "^3.8",
    "phpstan/phpstan": "^2.0",
    "squizlabs/php_codesniffer": "^3.7",
    "slevomat/coding-standard": "^8.15"
  },
  "autoload": {
    "psr-4": {
      "PHRETS\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "scripts": {
    "stan": [
      "phpcs --warning-severity=0",
      "phpstan"
    ],
    "test": "phpunit"
  }
}
